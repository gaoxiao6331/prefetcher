{
    "name": "prefetcher",
    "version": "1.0.0",
    "description": "Prefetcher service with Fastify and TypeScript",
    "main": "dist/index.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=dev nodemon",
        "build": "tsc -p tsconfig.build.json && tsc-alias",
        "start": "node dist/index.js start",
        "tool": "ts-node -r tsconfig-paths/register --files ./src/index.ts start",
        "run:prod": "pnpm build && cross-env NODE_ENV=prod pnpm start",
        "clean": "rm -rf dist",
        "format": "biome format --write .",
        "fix": "biome check --fix .",
        "test": "cross-env NODE_ENV=development jest --verbose"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@fastify/sensible": "^5.5.0",
        "axios": "^1.13.2",
        "chalk": "^4.1.2",
        "commander": "^14.0.2",
        "fastify": "^4.26.1",
        "fastify-metrics": "^10.0.0",
        "fastify-plugin": "^5.1.0",
        "fastify-type-provider-zod": "1.1.9",
        "pino": "^8.19.0",
        "pino-roll": "^4.0.0",
        "puppeteer": "^24.27.0",
        "zod": "3.23.8"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.3.8",
        "@types/jest": "^30.0.0",
        "@types/node": "^20.11.19",
        "cross-env": "^10.1.0",
        "jest": "^30.2.0",
        "nodemon": "^3.0.3",
        "pino-pretty": "^10.3.1",
        "ts-jest": "^29.4.6",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.16",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.3.3"
    },
    "pnpm": {
        "overrides": {
            "zod-to-json-schema": "3.23.5"
        }
    }
}